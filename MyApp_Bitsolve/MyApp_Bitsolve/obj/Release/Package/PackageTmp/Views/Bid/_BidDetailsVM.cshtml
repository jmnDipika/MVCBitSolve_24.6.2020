@model BusinessEntities.BidMasterVM

<table id="BidDetailsList" class="table table-responsive table-striped table-bordered" style="width: 100%">
    <thead class="alert-warning ">
        <tr>
            <th class="hidden">BidDetailId
            </th>
            <th class="hidden">BidId
            </th>
             <th class="hidden">RfqDetailsId
            </th>
            <th class="hidden">ItemId
            </th>
             <th style="width:300px">ItemName
            </th>
            <th style="width:200px">HSN
            </th>
            <th style="width:200px">Description
            </th>
            <th style="width:200px">ManufacturedBy
            </th>
            <th style="width:100px">Qty
            </th>
            <th class="hidden">UnitId
            </th>
            <th style="width:50px">Unit
            </th>
            <th style="width:100px">Price
            </th>
            <th class="hidden">TaxId
            </th>
            <th style="width:200px">Tax
            </th>
            <th style="width:100px">SubTotal
            </th>
        </tr>
    </thead>
    <tbody>
        @if (Model._BidDetailsVMList!=null)
        {
            foreach (var item in Model._BidDetailsVMList)
            {
            <tr>
                <td class="hidden">
                    @Html.DisplayFor(modelItem => item.BidDetailId)
                </td>
                <td class="hidden">
                    @Html.DisplayFor(modelItem => item.BidId)
                </td>
                 <td class="hidden">
                    @Html.DisplayFor(modelItem => item.RfqDetailsId)
                </td>
                <td class="hidden">
                    @Html.DisplayFor(modelItem => item.ItemId)
                </td>
                <td >
                    @Html.DisplayFor(modelItem => item.ItemName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.HSN)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Description)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ManufacturedBy)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Qty)
                </td>
                <td class="hidden">
                    @Html.DisplayFor(modelItem => item.UnitId)
                </td>
                 <td>
                    @Html.DisplayFor(modelItem => item.Unit)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Price)
                </td>
                <td class="hidden">
                    @Html.DisplayFor(modelItem => item.TaxId)
                </td>
                 <td>
                    @Html.DisplayFor(modelItem => item.Tax)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.SubTotal)
                </td>
               
            </tr>
            }
        }
    </tbody>
</table>
